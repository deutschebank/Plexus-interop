{
  "packages": ["packages/*"],
  "npmClient": "npm",
  "version": "0.1.0",
  "command": {
    "init": {
      "exact": true
    },
    "bootstrap": {
      "hoist": false
    },
    "version": {
      "exact": true,
      "yes": true,
      "push": false,
      "changelog": false,
      "gitTagVersion": false
    },
    "publish": {
      "yes": true,
      "fromPackage": true,
      "verifyAccess": false
    }
  }
}
