{
  "packages": ["packages/*"],
  "npmClient": "npm",
  "version": "0.1.0",
  "useWorkspaces": true,
  "command": {
    "init": {
      "exact": true
    },
    "version": {
      "exact": true,
      "yes": true,
      "push": false,
      "changelog": false,
      "gitTagVersion": false
    },
    "publish": {
      "yes": true,
      "fromPackage": true,
      "verifyAccess": false
    }
  },
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "useNx": false
}
