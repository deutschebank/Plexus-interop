{
  "private": true,
  "name": "@plexus-interop/web",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "Plexus Web Interop root module",
  "scripts": {
    "preinstall": "node ./scripts/replace-lock-registry.js && node ./scripts/replace-auth-vars.js --install",
    "postinstall": "node ./scripts/replace-lock-registry.js --backward && link-parent-bin",
    "build": "npm run build-ts -- --force && npm run build-bundles",
    "prebuild-ts": "lerna run prebuild --stream",
    "build-ts": "tsc -b packages",
    "build-bundles": "lerna run build:bundle --stream",
    "watch": "tsc -b -w packages --preserveWatchOutput",
    "prepublish-all": "node ./scripts/replace-auth-vars.js --publish && lerna exec -- cross-var shx cp $LERNA_ROOT_PATH/.npmrc .npmrc",
    "publish-all": "lerna publish from-package --loglevel=verbose",
    "postpublish-all": "shx rm -rf .npmrc && lerna exec -- shx rm -rf .npmrc",
    "lint": "lerna run --stream lint",
    "test": "lerna run --stream test",
    "verify": "npm run lint && npm run test",
    "e2e-native": "cd ./packages/e2e && npm run e2e-native",
    "poste2e-native": "npm run coverage",
    "e2e-web": "cd ./packages/e2e && npm run e2e-web-electron",
    "poste2e-web": "npm run coverage",
    "coverage": "nyc report --reporter=text --reporter=html",
    "build-e2e": "run-s build e2e-native e2e-web",
    "ci-build": "run-s build",
    "ci-build-e2e": "run-s build e2e-native e2e-web",
    "ci-publish": "run-s bump-version build publish-all",
    "bump-version": "cross-var lerna version $PLEXUS_BUILD_NPM_PACKAGE_VERSION --force-publish=* --loglevel=verbose",
    "benchmarks": "cd ./packages/e2e && npm run native-broker-benchmarks && npm run web-broker-benchmarks"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "^13.2.3",
    "@angular/compiler-cli": "^13.2.2",
    "@angular/language-service": "^13.2.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/chai": "^4.0.2",
    "@types/core-js": "^0.9.43",
    "@types/fs-extra": "5.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/jest": "^27.4.0",
    "@types/jstorage": "^0.3.30",
    "@types/long": "^4.0.0",
    "@types/mocha": "^2.2.43",
    "@types/node": "^12.11.1",
    "@types/rx": "^4.1.1",
    "@types/superagent": "^3.5.6",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "babel-register": "^6.23.0",
    "chai": "^4.1.0",
    "chokidar": "^1.7.0",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.3",
    "cross-var": "1.1.0",
    "electron": "1.8.8",
    "electron-log": "^2.2.9",
    "electron-mocha": "^4.0.2",
    "electron-packager": "^13.1.1",
    "eslint": "^8.2.0",
    "espree": "^3.4.3",
    "find-port": "^2.0.1",
    "glob": "^7.2.0",
    "glob-to-regexp": "^0.4.1",
    "http-server": "^0.11.1",
    "if-env": "1.0.0",
    "is-electron-renderer": "^2.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-reporters": "^2.2.0",
    "jasmine-spec-reporter": "~4.1.0",
    "jest": "^27.5.1",
    "jest-jasmine2": "^27.5.1",
    "jest-preset-angular": "^11.1.0",
    "karma": "~6.3.15",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^5.0.0",
    "lerna": "^4.0.0",
    "link-parent-bin": "^2.0.0",
