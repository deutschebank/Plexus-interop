    "nyc": "^11.2.1",
    "path": "^0.12.7",
    "protobufjs": "6.11.2",
    "protractor": "~7.0.0",
    "prr": "1.0.1",
    "renamer": "^0.6.1",
    "rxjs": "^7.5.4",
    "shx": "0.2.2",
    "sinon": "^2.4.1",
    "strip-bom": "^3.0.0",
    "trash": "^4.1.0",
    "trash-cli": "^1.4.0",
    "tree-kill": "^1.1.0",
    "ts-jest": "^27.1.3",
    "ts-mockito": "2.0.2",
    "ts-node": "~3.2.0",
    "tslib": "^2.3.1",
    "tslint": "5.8.0",
    "typescript": "~4.4.4",
    "watchify": "^3.9.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@plexus-interop/broker": "file:packages/broker",
    "@plexus-interop/ccy-pair-rate-provider": "file:packages/ccy-pair-rate-provider",
    "@plexus-interop/ccy-pair-rate-viewer": "file:packages/ccy-pair-rate-viewer",
    "@plexus-interop/cli": "file:packages/cli",
    "@plexus-interop/client": "file:packages/client",
    "@plexus-interop/client-api": "file:packages/client-api",
    "@plexus-interop/common": "file:packages/common",
    "@plexus-interop/common-api-impl": "file:packages/common-api-impl",
    "@plexus-interop/e2e": "file:packages/e2e",
    "@plexus-interop/electron-app-launcher": "file:packages/electron-launcher",
    "@plexus-interop/io": "file:packages/io",
    "@plexus-interop/metadata": "file:packages/metadata",
    "@plexus-interop/protocol": "file:packages/protocol",
    "@plexus-interop/remote": "file:packages/remote",
