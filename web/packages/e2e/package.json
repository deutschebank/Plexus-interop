{
  "name": "@plexus-interop/e2e",
  "version": "0.1.0",
  "types": "dist/main/src/index.d.ts",
  "main": "dist/main/src/index.js",
  "description": "Integration tests for Plexus Web Interop",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run gen-echo-protocol",
    "build": "npm run build:main && npm run build:bundle",
    "build:main": "tsc",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:bundle": "npm run copy-assets && npm run build-host-proxy",
    "unit": "jest --coverage",
    "test": "if-env PLEXUS_E2E_TESTS_ENABLED=true && npm run unit || echo 'E2E unit tests disabled'",
    "gen-echo-js": "pbjs --force-long -t static-module -r echo-client -w commonjs -o src/echo/gen/plexus-messages.js src/echo/metadata/messages.proto",
    "gen-echo-ts": "pbts --force-long -o src/echo/gen/plexus-messages.d.ts src/echo/gen/plexus-messages.js",
    "gen-echo-protocol": "npm run gen-echo-js && npm run gen-echo-ts",
    "copy-assets": "copyfiles src/echo/gen/* dist/main && copyfiles src/views/* dist/main",
    "build-host-proxy": "node scripts/bundle-all.js --standalone proxyHost --inputGlob dist/main/tests/common/TestCrossDomainHost.js --outputFile ./dist/main/tests/web/CrossDomainHost.bundle.js",
    "prepare-native-e2e-debug-tests": "node scripts/bundle-all.js --inputGlob dist/main/tests/native/*Connect*spec.js --outputFile ./dist/main/tests/native/RendererProcess.bundle.js",
    "e2e-web-electron": "node scripts/web-e2e-tests-launcher.js --file ./dist/main/tests/web/RendererProcess.bundle.js",
    "pree2e-web-electron": "node scripts/bundle-all.js --brokerCoverage true --inputGlob dist/main/tests/web/*spec.js --outputFile ./dist/main/tests/web/RendererProcess.bundle.js",
    "e2e-web-ie": "node scripts/web-e2e-tests-launcher.js --browser ie",
    "e2e-web-chrome": "node scripts/web-e2e-tests-launcher.js --browser chrome",
    "web-broker-benchmarks": "node scripts/web-e2e-tests-launcher.js --file ./dist/main/tests/web/benchmarks/RendererProcessBenchmarks.bundle.js",
    "preweb-broker-benchmarks": "node scripts/bundle-all.js --inputGlob dist/main/tests/web/benchmarks/*spec.js --outputFile ./dist/main/tests/web/benchmarks/RendererProcessBenchmarks.bundle.js",
    "native-broker-benchmarks": "node scripts/native-e2e-tests-launcher.js --file ./dist/main/tests/native/benchmarks/RendererProcessBenchmarks.bundle.js",
    "prenative-broker-benchmarks": "node scripts/bundle-all.js --inputGlob dist/main/tests/native/benchmarks/*spec.js --outputFile ./dist/main/tests/native/benchmarks/RendererProcessBenchmarks.bundle.js",
    "e2e-native": "node scripts/native-e2e-tests-launcher.js --file ./dist/main/tests/native/RendererProcess.bundle.js",
    "pree2e-native": "node scripts/bundle-all.js --clientCoverage true --inputGlob dist/main/tests/native/*spec.js --outputFile ./dist/main/tests/native/RendererProcess.bundle.js",
    "e2e-native-debug": "npm run build && npm run prepare-native-e2e-debug-tests && npm run e2e-native-debug-run",
    "e2e-native-debug-run": "node scripts/native-e2e-tests-launcher.js --printBrokerStdout true --debug true --file ./dist/main/tests/native/RendererProcess.bundle.js"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@plexus-interop/broker": "file:../broker",
    "@plexus-interop/client": "file:../client",
    "@plexus-interop/client-api": "file:../client-api",
    "@plexus-interop/common": "file:../common",
    "@plexus-interop/common-api-impl": "file:../common-api-impl",
    "@plexus-interop/io": "file:../io",
    "@plexus-interop/metadata": "file:../metadata",
    "@plexus-interop/protocol": "file:../protocol",
    "@plexus-interop/remote": "file:../remote",
    "@plexus-interop/transport-common": "file:../transport-common",
    "@plexus-interop/websocket-transport": "file:../websocket-transport",
    "@types/long": "^4.0.0",
    "chai": "^4.1.0",
    "core-js": "^2.5.3",
    "is-electron-renderer": "^2.0.1",
    "long": "^4.0.0",
    "protobufjs": "6.11.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testRunner": "jest-jasmine2",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jasmine.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
