{
  "name": "@plexus-interop/websocket-transport",
  "description": "Web Socket based Transport for Plexus Web Interop",
  "license": "Apache-2.0",
  "version": "0.1.0",
  "sideEffects": false,
  "main": "./dist/main/src/index.js",
  "module": "./dist/esm5/src/index.js",
  "es2015": "./dist/esm/src/index.js",
  "types": "./dist/main/src/index.d.ts",
  "exports": {
    ".": {
      "node": "./dist/main/src/index.js",
      "es2015": "./dist/esm/src/index.js",
      "default": "./dist/esm5/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run build:main",
    "build:main": "tsc",
    "unit": "jest --coverage",
    "test": "npm run unit"
  },
  "dependencies": {
    "@plexus-interop/common": "file:../common",
    "@plexus-interop/protocol": "file:../protocol",
    "@plexus-interop/transport-common": "file:../transport-common",
    "long": "^4.0.0",
    "protobufjs": "7.1.2",
    "reflect-metadata": "^0.1.10",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "rxjs": "^7.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRunner": "jest-jasmine2",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jasmine.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
