{
  "name": "@plexus-interop/client",
  "version": "0.1.0",
  "description": "Client for Plexus Interop",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/main/src/index.js",
  "module": "./dist/esm5/src/index.js",
  "es2015": "./dist/esm/src/index.js",
  "types": "./dist/main/src/index.d.ts",
  "exports": {
    ".": {
      "node": "./dist/main/src/index.js",
      "es2015": "./dist/esm/src/index.js",
      "default": "./dist/esm5/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run build:main",
    "build:main": "tsc",
    "watch": "tsc -w",
    "test": "jest --coverage --forceExit",
    "gen-client-js": "pbjs --force-long -t static-module -r client -w commonjs -o src/gen/internal-client-protocol.js ../../../protocol/Plexus.Interop.proto",
    "gen-client-ts": "pbts --force-long -o src/gen/internal-client-protocol.d.ts src/gen/internal-client-protocol.js",
    "gen-client-protocol": "npm run gen-client-js && npm run gen-client-ts"
  },
  "dependencies": {
    "@plexus-interop/client-api": "file:../client-api",
    "@plexus-interop/common": "file:../common",
    "@plexus-interop/io": "file:../io",
    "@plexus-interop/protocol": "file:../protocol",
    "@plexus-interop/transport-common": "file:../transport-common",
    "@plexus-interop/websocket-transport": "file:../websocket-transport",
    "@types/long": "^4.0.0",
    "loglevel": "^1.4.1",
    "long": "^4.0.0",
    "protobufjs": "6.11.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^7.5.4",
    "tslib": "^2.3.1",
    "typescript-collections": "1.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRunner": "jest-jasmine2",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jasmine.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
