{
  "name": "@plexus-interop/studio",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm run test",
    "build": "ng build && npm run copy-to-bin",
    "test": "jest",
    "lint": "echo 'Not implemented'",
    "copy-to-bin": "copyfiles dist/* ../../../bin/win-x86/broker/studio -u 1",
    "clear-jest": "jest --clearCache",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/router-store": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "@plexus-interop/broker": "file:../broker",
    "@plexus-interop/client": "file:../client",
    "@plexus-interop/client-api": "file:../client-api",
    "@plexus-interop/common": "file:../common",
    "@plexus-interop/io": "file:../io",
    "@plexus-interop/metadata": "file:../metadata",
    "@plexus-interop/protocol": "file:../protocol",
    "@plexus-interop/transport-common": "file:../transport-common",
    "@plexus-interop/websocket-transport": "file:../websocket-transport",
    "@types/core-js": "^0.9.43",
    "arraybuffer-slice": "^0.1.2",
    "bootstrap": "^4.1.0",
    "bootswatch": "^4.1.0",
    "core-js": "^2.5.3",
    "font-awesome": "^4.7.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "rxjs": "^7.5.2",
    "typescript-collections": "1.2.3",
    "zone.js": "^0.8.14"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-jest.ts",
    "transform": {
      "^.+\\.(ts|js|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-preset-angular/AngularSnapshotSerializer.js",
      "<rootDir>/node_modules/jest-preset-angular/HTMLCommentSerializer.js"
    ]
  }
}
