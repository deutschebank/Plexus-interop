{
  "name": "@plexus-interop/common-api-impl",
  "description": "Common API implementation",
  "license": "Apache-2.0",
  "version": "0.1.0",
  "sideEffects": false,
  "main": "./dist/main/src/index.js",
  "module": "./dist/esm5/src/index.js",
  "es2015": "./dist/esm/src/index.js",
  "types": "./dist/main/src/index.d.ts",
  "scripts": {
    "build": "npm run build:main && npm run build:bundle",
    "build:main": "tsc",
    "build:bundle": "webpack",
    "build:watch": "tsc -w",
    "test": "jest --coverage --passWithNoTests"
  },
  "dependencies": {
    "@plexus-interop/client": "file:../client",
    "@plexus-interop/client-api": "file:../client-api",
    "@plexus-interop/common": "file:../common",
    "@plexus-interop/io": "file:../io",
    "@plexus-interop/metadata": "file:../metadata",
    "@plexus-interop/protocol": "file:../protocol",
    "@plexus-interop/remote": "file:../remote",
    "@plexus-interop/transport-common": "file:../transport-common",
    "@plexus-interop/websocket-transport": "file:../websocket-transport",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "rxjs": "^7.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRunner": "jest-jasmine2",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jasmine.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
