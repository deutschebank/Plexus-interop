{
  "name": "@plexus-interop/ccy-pair-rate-provider",
  "version": "0.1.0",
  "description": "Quick start Rate Provider App",
  "main": "index.js",
  "private": true,
  "license": "Apache-2.0",
  "dependencies": {
    "@plexus-interop/client": "file:../client",
    "@plexus-interop/common": "file:../common",
    "@plexus-interop/transport-common": "file:../transport-common",
    "@plexus-interop/websocket-transport": "file:../websocket-transport",
    "protobufjs": "6.11.3"
  },
  "scripts": {
    "build:main": "tsc",
    "copy-assets": "copyfiles src/gen/* dist/main && copyfiles -f src/views/* dist",
    "prebuild:bundle": "npm run copy-assets",
    "build:bundle": "webpack",
    "build": "npm run build:main && npm run copy-assets && npm run build:bundle",
    "postbuild:bundle": "copyfiles ./dist/* -f ../../../bin/win-x86/samples/quick-start/apps/WebCcyPairRateProvider"
  }
}
