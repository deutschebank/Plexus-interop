{
  "name": "@plexus-interop/transport-common",
  "version": "0.1.0",
  "types": "dist/main/src/index.d.ts",
  "main": "dist/main/src/index.js",
  "description": "General Transport for Plexus Web Interop",
  "license": "Apache-2.0",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:main",
    "build:watch": "tsc -w",
    "build:main": "tsc -p tsconfig.json",
    "lint": "tslint src/**/*.ts",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@plexus-interop/common": "file:../common",
    "@plexus-interop/protocol": "file:../protocol",
    "@types/loglevel": "^1.4.29",
    "@types/long": "^4.0.0",
    "loglevel": "^1.4.1",
    "long": "^4.0.0",
    "protobufjs": "6.11.2",
    "reflect-metadata": "^0.1.10",
    "tslib": "^2.3.1",
    "typescript-collections": "1.2.3"
  },
  "peerDependencies": {
    "protobufjs": "6.11.2",
    "rxjs": "^7.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRunner": "jest-jasmine2",
    "setupFilesAfterEnv": ["<rootDir>/setup-jasmine.js"],
    "testMatch": ["<rootDir>/tests/**/*.spec.ts"],
    "collectCoverageFrom": ["src/**/*.ts", "!src/**/*.d.ts"]
  }
}
