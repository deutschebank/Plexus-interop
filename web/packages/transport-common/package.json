{
  "name": "@plexus-interop/transport-common",
  "version": "0.1.0",
  "types": "dist/main/src/index.d.ts",
  "main": "dist/main/src/index.js",
  "description": "General Transport for Plexus Web Interop",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "yarn lint && trash dist",
    "build": "yarn build:main",
    "postbuild": "yarn test",
    "build:watch": "tsc -w",
    "build:main": "tsc -p tsconfig.json",
    "lint": "tslint src/**/*.ts",
    "test": "jest --coverage --runInBand"
  },
  "dependencies": {
    "rxjs": "7.5.2",
    "protobufjs": "6.11.2",
    "@types/loglevel": "^1.4.29",
    "@types/long": "^4.0.0",
    "@plexus-interop/common": "0.1.0",
    "@plexus-interop/protocol": "0.1.0",
    "loglevel": "^1.4.1",
    "long": "^4.0.0",
    "reflect-metadata": "^0.1.10",
    "tslib": "^1.10.0",
    "typescript-collections": "1.2.3"
  },
  "peerDependencies": {
    "protobufjs": "6.11.2",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@types/long": "^4.0.0",
    "@types/chai": "^4.0.2",
    "@types/jest": "^23.1.3",
    "@types/mocha": "^2.2.43",
    "@types/node": "^7.0.5",
    "babel-register": "^6.23.0",
    "browserify": "^14.5.0",
    "chai": "^4.1.0",
    "copyfiles": "^1.2.0",
    "glob": "^7.1.2",
    "jasmine-reporters": "^2.2.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.0.0",
    "minimist": "^1.2.0",
    "mocha": "^3.5.0",
    "path": "^0.12.7",
    "sinon": "^2.4.1",
    "trash": "^4.1.0",
    "trash-cli": "^1.4.0",
    "ts-mockito": "2.0.2",
    "tslint": "5.8.0",
    "typescript": "3.7.2",
    "watchify": "^3.9.0",
    "yarn": "^1.17.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jasmine.js",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": ".+/dist/main/tests/.+.spec.js$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "dist/main/src/**/*.js",
      "!dist/main/src/gen/**/*.js",
      "!src/*.d.ts",
      "!src/**/*.d.ts",
      "!src/**/*.spec.ts"
    ]
  }
}
