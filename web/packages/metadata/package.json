{
  "name": "@plexus-interop/metadata",
  "description": "Plexus Interop Metadata management",
  "license": "Apache-2.0",
  "version": "0.1.0",
  "main": "./dist/main/src/index.js",
  "module": "./dist/esm5/src/index.js",
  "es2015": "./dist/esm/src/index.js",
  "types": "./dist/main/src/index.d.ts",
  "scripts": {
    "build": "npm run build:main",
    "build:main": "tsc",
    "build:watch": "tsc -w",
    "unit": "jest --coverage",
    "test": "npm run unit"
  },
  "dependencies": {
    "@plexus-interop/common": "file:../common",
    "@plexus-interop/protocol": "file:../protocol",
    "@plexus-interop/remote": "file:../remote"
  },
  "peerDependencies": {
    "rxjs": "^7.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRunner": "jest-jasmine2",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jasmine.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
