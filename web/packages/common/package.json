{
  "name": "@plexus-interop/common",
  "version": "1.0.0",
  "sideEffects": true,
  "description": "Common functionality for Plexus Interop Web",
  "license": "Apache-2.0",
  "main": "./dist/main/src/index.js",
  "module": "./dist/esm5/src/index.js",
  "es2015": "./dist/esm/src/index.js",
  "types": "./dist/main/src/index.d.ts",
  "exports": {
    ".": {
      "node": "./dist/main/src/index.js",
      "es2015": "./dist/esm/src/index.js",
      "default": "./dist/esm5/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run build:main",
    "build:main": "tsc",
    "build:watch": "tsc -w",
    "unit": "jest --coverage",
    "test": "npm run unit"
  },
  "dependencies": {
    "@types/long": "^4.0.0",
    "loglevel": "^1.4.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "long": "^4.0.0",
    "reflect-metadata": "^0.1.10",
    "tslib": "^2.3.1",
    "typescript-collections": "1.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRunner": "jest-jasmine2",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jasmine.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
