{
  "name": "@plexus-interop/common",
  "version": "0.1.0",
  "sideEffects": false,
  "types": "dist/main/src/index.d.ts",
  "main": "dist/main/src/index.js",
  "description": "Common functionality for Plexus Interop Web",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run build:main",
    "build:main": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "lint": "tslint src/**/*.ts",
    "unit": "jest --coverage",
    "test": "npm run unit"
  },
  "dependencies": {
    "@types/long": "^4.0.0",
    "loglevel": "^1.4.1",
    "loglevel-plugin-prefix": "^0.5.3",
    "long": "^4.0.0",
    "reflect-metadata": "^0.1.10",
    "tslib": "^2.3.1",
    "typescript-collections": "1.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRunner": "jest-jasmine2",
    "setupFilesAfterEnv": ["<rootDir>/setup-jasmine.js"],
    "testMatch": ["<rootDir>/tests/**/*.spec.ts"],
    "collectCoverageFrom": ["src/**/*.ts", "!src/**/*.d.ts"]
  }
}
